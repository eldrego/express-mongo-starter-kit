{
  "name": "trial",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test concurrently \"npm run client-test\" \"\"",
    "server-test": "cross-env NODE_ENV=test nyc _mocha server/test --exit",
    "start": "cross-env NODE_ENV=production npm run server-prod",
    "dev": "cross-env NODE_ENV=development npm run server-dev",
    "server-prod": "npm run build && node ./build/index.js",
    "server-dev": "cross-env NODE_ENV=development nodemon index.js --exec babel-node",
    "server-build": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run server-build",
    "clean": "rm -rf build && mkdir build",
    "open-cov": "open coverage/lcov-report/index.html"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.18",
    "prop-types": "^15.7.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "@babel/runtime": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codecov": "^3.7.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "html-webpack-plugin": "^4.3.0",
    "mocha": "^10.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2"
  }
}
